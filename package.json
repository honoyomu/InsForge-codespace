{
  "name": "insforge",
  "version": "1.0.0",
  "description": "Open source backend-as-a-service with PostgreSQL and MCP integration",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "concurrently -n \"backend,frontend\" -c \"cyan,magenta\" \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:debug": "concurrently -n \"backend,frontend\" -c \"cyan,magenta\" \"cross-env DEBUG_MODE=true npm run dev:backend\" \"cd frontend && cross-env VITE_DEBUG_MODE=true npm run dev\"",
    "dev:backend": "tsx watch src/server.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:mcp": "tsx watch src/mcp/index.ts",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc",
    "build:frontend": "cd frontend && npm install && npm run build",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "mcp": "node dist/mcp/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "./tests/run-all-tests.sh",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:all": "npm run lint && cd frontend && npm run lint",
    "format": "prettier --write src",
    "format:check": "prettier --check src"
  },
  "keywords": [
    "backend-as-a-service",
    "postgresql",
    "mcp",
    "jwt"
  ],
  "author": "Insforge",
  "license": "MIT",
  "dependencies": {
    "@insforge/insforge-mcp": "^1.0.8",
    "@modelcontextprotocol/sdk": "^1.15.1",
    "@types/multer": "^1.4.13",
    "@types/pg": "^8.15.4",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "commander": "^14.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "form-data": "^4.0.3",
    "google-auth-library": "^10.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "etcd3": "^1.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.11.24",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "@vitest/coverage-v8": "^3.2.4",
    "c8": "^10.1.3",
    "concurrently": "^8.2.2",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.3",
    "globals": "^13.24.0",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.5",
    "supertest": "^7.1.3",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.38.0",
    "vitest": "^3.2.4"
  }
}
