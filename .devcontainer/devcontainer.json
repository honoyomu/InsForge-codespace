{
  "name": "InsForge Demo",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "insforge",
  "workspaceFolder": "/app",
  "envFile": "../.env",
  "extensions": [
    "saoudrizwan.claude-dev"
  ],
  "postStartCommand": "docker compose up",
  "forwardPorts": [7130, 7131],
  "portsAttributes": {
    "7130": {
      "visibility": "public",
      "label": "InsForge API"
    },
    "7131": {
      "visibility": "public", 
      "label": "InsForge Dashboard"
    }
  }
}